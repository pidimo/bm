<Dependency>
<Name>MS XML 3.0 SP7</Name>
<RuntimeTempFolder>msxml3sp7</RuntimeTempFolder>
<RevisionDate>Monday, November 17, 2008</RevisionDate>
<Description>Installs Microsoft XML 3.0 SP7 parser</Description>
<DetectScript>function IsMSXMLSP7Installed()
	-- Get the version of file msxml3.dll
	local strXMLRuntimeFile = _SystemFolder.."\\msxml3.dll";
	
	if(not File.DoesExist(strXMLRuntimeFile))then

		-- log: msxml3.dll could not be found, so the runtime will be installed.
		SetupData.WriteToLogFile("Info\tMSXML3 SP7 Module: No version of msxml3.dll was found. The MSXML3 SP7 runtime will be installed.\r\n", true);
	
		-- msxml3.dll is not installed, so install the module.
		return false;
	end
	
	tblFileInfo = File.GetVersionInfo(strXMLRuntimeFile);
	if(not tblFileInfo)then
	
		-- log: couldn't get version info for msxml3.dll, so the runtime will be installed.
		SetupData.WriteToLogFile("Info\tMSXML3 SP7 Module: Unable to get version info for msxml3.dll. The MSXML3 SP7 runtime will be installed.\r\n", true);
	
		-- The version information could not be retrieved from msxml3.dll, so install the run time.
		return false;
	end

		
	local nCompareResult = String.CompareFileVersions(tblFileInfo.FileVersion,"8.70.1104.0");
	if(nCompareResult == -1)then
	
		-- log: looks like the msxml3.dll is OFN
		SetupData.WriteToLogFile("Info\tMSXML3 SP7: Older version of msxml3.dll detected. The MSXML3 SP7 runtime will be installed.\r\n", true);
	
		-- The installed version is older than the newest version, so install the module.
		return false;
	else

		-- log: looks like the msxml3.dll is up to date
		SetupData.WriteToLogFile("Info\tMSXML3 SP7: Current version of msxml3.dll detected. The MSXML3 SP7 runtime is already installed.\r\n", true);

		-- The installed version is up to date, so don't install the module.
		return true;
	end
end
</DetectScript>
<DetectFunction>IsMSXMLSP7Installed</DetectFunction>
<InstallScript>-- Variables used in the installation actions:
local strMessage = 
[[Setup has detected that your Microsoft XML Parser 3.0 run-time files are out of date.
Click OK to install this technology now or Cancel to abort the setup.]];

local strDialogTitle = "Technology Required";
local bShowUserPrompt = false; -- set this to true to ask user whether to install the module
local bRunInstallFile = true; -- sets default choice (whether to run the setup by default)
local strRuntimeSupportFolder = SessionVar.Expand("%TempLaunchFolder%\\msxml3sp7");
local strFileToRun = strRuntimeSupportFolder.."\\msxml3.msi";

-----------------------------------------------------------------------------------------

-- log: the MSXML 3 installation has started.
SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: Installation script started.\r\n", true);

-- Delete temporary file and runtime support folder (with error checking)
local function PerformCleanUp()

	File.Delete(strFileToRun);

	error = Application.GetLastError();
	if error ~= 0 then
		SetupData.WriteToLogFile("Error\tMSXML3 SP7 Module: Unable to delete temp file (".._tblErrorMessages[error]..")\r\n", true);
	end

	Folder.Delete(strRuntimeSupportFolder);

	error = Application.GetLastError();
	if error ~= 0 then
		SetupData.WriteToLogFile("Error\tMSXML3 SP7 Module: Unable to delete temp folder (".._tblErrorMessages[error]..")\r\n", true);
	end
end

-- Check if the dialog should be displayed asking whether or not they want to install the module.
if(bShowUserPrompt)then
	local nDialogResult = Dialog.Message(strDialogTitle,strMessage,MB_OKCANCEL,MB_ICONEXCLAMATION);
	if(nDialogResult == IDOK)then
		-- The user chose to install the module.
		bRunInstallFile = true;

		-- log: the user wants to install it
		SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: User confirmation received, we are ready to install.\r\n", true);

	else
		-- The user chose not to install the module.
		bRunInstallFile = false;

		-- log: the user said no
		SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: User chose NOT to install the module.\r\n", true);
	end
end


-- Check if the user wants to install the runtime.
if(bRunInstallFile)then

	-- log: go go go!
	SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: Running MS XML 3 installer.\r\n", true);

	-- The "/R:N" command line switch tells the MSXML install not to show the
	-- reboot screen if necessary. We will detect that through the setup's
	-- process return code and set the internal _NeedsReboot variable if necessary
	-- Note: You can add the command line swicth "/Q" to run the install in silent mode
	--local nResult = File.Run(strFileToRun,"","",SW_SHOWNORMAL);
	strCommandLine = "/norestart /passive /i \""..strFileToRun.."\"";
	local nResult = MSI.RunMsiexec(strCommandLine, true);
	
	if(nResult == 3010)then
		--  install indicated that it needs reboot to be complete
		-- Set Setup Factory's reboot variable so that the reboot is just
		-- performed at the end of the install
		_NeedsReboot = true;

		-- log: need to reboot
		SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: MSXML3 SP7 installer has signalled that it needs to reboot.\r\n", true);

	end
	
	-- Delete the runtime installer file and remove the temp folder
	PerformCleanUp();
	
	-- log: all done!
	SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: MSXML3 SP7 runtime installation complete.\r\n", true);
	
else
	-- The user chose not to install the runtime so delete the run time installer file,
	-- remove the temp folder and then exit the setup.
	PerformCleanUp();

	-- log: all done!
	SetupData.WriteToLogFile("Success\tMSXML3 SP7 Module: MSXML3 SP7 runtime was not installed.\r\n", true);

	Application.Exit();

end
</InstallScript>
