<Script>function getWordRunningWarningMessage()

	-- These actions are performed before the screen is shown.
	local installerLanguage = Application.GetInstallLanguage();
	
	--English/9
	if (installerLanguage.Primary == 9) then
		return {title="Warning", message="The program \"Microsoft Word\" must be closed in order to continue with the installation.\n\nPlease close the program and press Retry to continue."};
	end
	--Spanish/10
	if (installerLanguage.Primary == 10) then
	   return {title="Aviso", message="El programa \"Microsoft Word\" debe ser cerrado para continuar con la instalación.\n\nPor favor cierre el programa y presione Reintentar para continuar."};
	end
	--German/7
	if (installerLanguage.Primary == 7) then
		return {title="Warnung", message="\"Microsoft Word\" muss geschlossen werden, um mit der Installation fortzufahren.\n\nBitte beenden Sie \"Microsoft Word\" und drücken Sie auf \"Wiederholen\"."};
	end
	
	--French/12
	if (installerLanguage.Primary == 12) then
    	return {title="Avertissement", message="Le programme \"Microsoft Word\" doit être fermé en vue de poursuivre l'installation.\n\nS'il vous plaît fermer le programme et appuyez sur Réessayer pour continuer."};
	end

 
end

function isWordRunning()

	-- The filename to check for.
	strFileToCheck = "winword.exe";
	-- Get all of the running processes.
	tbProcesses = Window.EnumerateProcesses(false);

	-- Search through the processes and check the file path who spawned the process.
	for j, file_path in tbProcesses do

    	-- Split the path into its components to get its filename.
    	tbPathPart = String.SplitPath(file_path);
    	-- Generate the complete filename.
    	strFilename = tbPathPart.Filename..tbPathPart.Extension;

    	if (String.CompareNoCase(strFilename, strFileToCheck)== 0) then
    		return true;
	   	end
	end
	
	return false;

end

function executeUtilExe(parameter)
AppDir = SessionVar.Expand("%AppFolder%");
File.Run(AppDir.."\\util.exe", parameter, SessionVar.Expand("%AppFolder%"), SW_SHOWNORMAL, true);
end

