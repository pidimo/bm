        </action>

        <action path="/Mail/Forward/ComposeEmail"
                type="com.piramide.elwis.web.bmapp.action.webmail.ComposeEmailForwardRESTAction"
                name="emailForm"
                scope="request"
                validate="false"
                parameter="/bmapp/webmail/ComposeMailCatalogsJson.jsp"
                className="com.piramide.elwis.web.common.mapping.CheckEntriesMapping"
                roles="MAIL.EXECUTE">
            <set-property property="mainTable" value="mail"/>
            <forward name="NoUserMail" path="/FailJson.jsp"/>
        </action>

        <action path="/Mail/ComposeEmail"
                type="com.piramide.elwis.web.bmapp.action.webmail.ComposeEmailRESTAction"
                name="emailForm"
                scope="request"
                input="/webmail/ReadEmailJson.jsp"
                validate="false"
                parameter="com.piramide.elwis.cmd.webmailmanager.ComposeMailCmd"
                roles="MAIL.EXECUTE">
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="SearchEmail" path="/SuccessJson.jsp"/>
            <forward name="Draft" path="/SuccessJson.jsp" redirect="true"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Mail/Reply"
                type="com.piramide.elwis.web.bmapp.action.webmail.ReplyRESTAction"
                name="emailForm"
                scope="request"
                input="/MainSearchJson.jsp"
                validate="false"
                parameter="com.piramide.elwis.cmd.webmailmanager.ReadEmailCmd">
            <forward name="Success" path="/webmail/ReadEmailJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Mail/Reply/Send"
                type="com.piramide.elwis.web.bmapp.action.webmail.ReplySendRESTAction"
                name="emailForm"
                scope="request"
                input="/webmail/ReadEmailJson.jsp"
                validate="false"
                parameter="com.piramide.elwis.cmd.webmailmanager.ComposeMailCmd"
                roles="MAIL.EXECUTE">
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="SearchEmail" path="/SuccessJson.jsp"/>
            <forward name="Draft" path="/SuccessJson.jsp" redirect="true"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Mail/ReadFolders" type="com.piramide.elwis.web.webmail.action.WebmailDefaultAction" name="form"
                scope="request" validate="false" parameter="com.piramide.elwis.cmd.webmailmanager.WebmailReadCmd"
                roles="MAIL.VIEW">
            <forward name="Success" path="/webmail/MailFoldersJson.jsp"/>
            <forward name="NoUserMail" path="/FailJson.jsp"/>
        </action>

        <action path="/WebmailNotifier/Read"
                type="com.piramide.elwis.web.webmail.action.WebmailNotifierDefaultAction"
                name="form"
                scope="request"
                validate="false"
                parameter="com.piramide.elwis.cmd.webmailmanager.WebmailNotifierCmd">
            <forward name="Success" path="/webmail/WebmailNotifierJson.jsp"/>
            <forward name="Fail" path="/webmail/WebmailNotifierJson.jsp"/>
        </action>

        <action path="/Mail/DeleteConfirmation"
                type="com.piramide.elwis.web.bmapp.action.webmail.DeleteConfirmationRESTAction"
                name="form"
                scope="request" validate="false"
                parameter="com.piramide.elwis.cmd.webmailmanager.MailTrayCmd"
                roles="MAIL.DELETE">
            <forward name="Success" path="/SuccessJson.jsp"/>
        </action>
