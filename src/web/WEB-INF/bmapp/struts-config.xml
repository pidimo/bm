        <form-bean name="participantImportForm" type="com.piramide.elwis.web.schedulermanager.form.ParticipantForm"/>
        <form-bean name="productForm" type="com.piramide.elwis.web.productmanager.form.ProductForm"/>
        <form-bean name="userChangeAppForm" type="com.piramide.elwis.web.admin.form.UserChangeAppForm"/>
        <form-bean name="companyInfoForm" type="net.java.dev.strutsejb.web.DefaultForm"/>
    </form-beans>

    <!--@@globalExceptionsXmlFragment@@-->
    <global-forwards>
        <forward name="MainSearch" path="/MainSearchJson.jsp"/>
    </global-forwards>

    <action-mappings type="org.apache.struts.config.SecureActionConfig">

        <action path="/LogonBMApp" type="com.piramide.elwis.web.bmapp.action.admin.LogonRESTAction" name="form" scope="request"
                input="/index.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.admin.LogonCmd">
            <forward name="Success" path="/LogonJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
            <forward name="ChangePassword" path="/FailJson.jsp"/>
        </action>

        <action path="/LogonBMApp/WVAPP" type="com.piramide.elwis.web.bmapp.action.admin.LogonWVAppRESTAction" name="form" scope="request"
                input="/index.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.admin.LogonCmd">
            <forward name="Success" path="/LogonJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
            <forward name="ChangePassword" path="/FailJson.jsp"/>
        </action>

        <action path="/LogoffBMApp" type="com.piramide.elwis.web.admin.action.LogoffAction"
                name="form"
                scope="request"
                validate="false">
            <forward name="Logoff" path="/SuccessJson.jsp"/>
        </action>

        <action path="/Reload/UserInfo" type="com.piramide.elwis.web.bmapp.action.admin.ReloadUserInfoRESTAction"
                name="form"
                scope="request"
                validate="false">
            <forward name="Success" path="/admin/ReloadUserInfoJson.jsp"/>
            <forward name="Fail" path="/SessionExpiredJson.jsp"/>
        </action>

        <action path="/Reload/WVApp/UserInfo" type="com.piramide.elwis.web.bmapp.action.admin.ReloadUserInfoWVAppRESTAction"
                name="form"
                scope="request"
                validate="false">
            <forward name="Success" path="/admin/ReloadUserInfoJson.jsp"/>
            <forward name="Fail" path="/SessionExpiredJson.jsp"/>
        </action>

		<action path="/TranslateREST"
			type="org.apache.struts.actions.ForwardAction"
			scope="request" parameter="/bmapp/translate.jsp"/>

        <!--start miky-->
        <action path="/Logon/Company/DownloadLogoImage"
                type="com.piramide.elwis.web.bmapp.action.DownloadCompanyLogoRESTAction" name="form"
                scope="request" validate="false"
                parameter="com.piramide.elwis.cmd.productmanager.DownloadProductImageCmd">
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Company/Info"
