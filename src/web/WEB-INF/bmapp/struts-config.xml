                parameter="webmail@mailTrayList">
            <forward name="Success" path="/webmail/MailTrayJsonList.jsp"/>
            <forward name="NoUserMail" path="/FailJson.jsp"/>
        </action>

        <action path="/Mail/ReadEmail"
                type="com.piramide.elwis.web.webmail.action.ReadEmailAction"
                name="emailForm"
                scope="request"
                input="/Mail/MailTray.do"
                validate="false"
