            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="Recurrence" path="/scheduler/AppointmentRecurrenceDeleteJson.jsp"/>
            <forward name="noRecurrence" path="/Appointment/Forward/Delete.do"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Appointment/View/Delete"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentRecurrenceDeleteRESTAction"
                name="appointmentForm"
                scope="request" input="/scheduler/AppointmentJson.jsp" validate="false" roles="APPOINTMENT.DELETE"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="Cancel" path="/SuccessJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Appointment/Forward/Delete"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentReadRESTAction"
                name="appointmentForm"
                scope="request"
                input="/FailJson.jsp"
                validate="false"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping"
                roles="APPOINTMENT.DELETE">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="Success" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
