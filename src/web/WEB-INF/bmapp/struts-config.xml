        <action path="/Mail/Ajax/Find/Recipients"
                type="com.piramide.elwis.web.webmail.action.MailFindRecipientsAjaxAction"
                name="form"
                scope="request"
                validate="false"
                roles="MAIL.EXECUTE">
        </action>


        <!--scheduler-->
        <action path="/Appointment/REST" type="com.piramide.elwis.web.bmapp.action.DefaultRESTAction" parameter="dto(appointmentId)"
                scope="request" validate="false">
            <forward name="List" path="/AppointmentView.do"/>
            <forward name="Get" path="/Appointment/Forward/Update.do"/>
            <forward name="Create" path="/error.jsp"/>
            <forward name="Update" path="/error.jsp"/>
            <forward name="Delete" path="/error.jsp"/>
        </action>

        <action path="/AppointmentView"
                type="com.piramide.elwis.web.schedulermanager.action.AppointmentUIAction"
                name="form"
                scope="request"
                validate="false"
                roles="APPOINTMENT.VIEW">
            <forward name="daily" path="/scheduler/AppointmentViewJson.jsp"/>
            <forward name="weekly" path="/scheduler/AppointmentViewJson.jsp"/>
            <forward name="monthly" path="/scheduler/AppointmentViewJson.jsp"/>
            <forward name="yearly" path="/scheduler/AppointmentViewJson.jsp"/>
            <forward name="fail" path="/AppointmentView.do"/>
        </action>

        <action path="/Appointment/Forward/Create"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentCreateForwardRESTAction"
                name="appointmentForm"
                scope="request"
                validate="false"
                input="/FailJson.jsp"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping"
                parameter="/scheduler/AppointmentDetailForCreate.jsp"
                roles="APPOINTMENT.CREATE">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="Success" path="/scheduler/AppointmentCatalogsJson.jsp"/>
            <forward name="redirect" path="/FailJson.jsp"/>
            <forward name="fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Appointment/Create"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentCreateRESTAction"
                name="appointmentForm"
                scope="request"
                input="/scheduler/AppointmentJson.jsp"
                validate="false"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping"
                roles="APPOINTMENT.CREATE">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="ReturnSearchList" path="/SuccessJson.jsp"/>
            <forward name="Reload" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="fail" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
            <forward name="Search" path="/FailJson.jsp"/>
        </action>

        <action path="/Appointment/Forward/Update"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentReadRESTAction"
                name="appointmentForm" scope="request" input="/AppointmentView.do" validate="false"
                roles="APPOINTMENT.VIEW"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="Success" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Appointment/Update"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentUpdateRESTAction" name="appointmentForm"
                scope="request" input="/scheduler/AppointmentJson.jsp" validate="false" roles="APPOINTMENT.UPDATE"
                className="org.alfacentauro.fantabulous.web.mapping.URLParameterReWriteMapping">
            <set-property property="paramReWriteText" value="calendar"/>
            <set-property property="paramReWriteText" value="type"/>
            <forward name="ReturnSearchList" path="/SuccessJson.jsp"/>
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="ConcurrencyFail" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Reload" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="fail" path="/scheduler/AppointmentJson.jsp"/>
            <forward name="Fail" path="/MainSearchJson.jsp"/>
        </action>

        <action path="/Appointment/Forward/View/Delete"
                type="com.piramide.elwis.web.bmapp.action.scheduler.AppointmentForwardDeleteRESTAction"
                name="appointmentForm"
                scope="request" input="/FailJson.jsp" validate="false" roles="APPOINTMENT.DELETE"
