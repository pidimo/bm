                scope="request" validate="false"
                input="/FailJson.jsp"
                parameter="com.piramide.elwis.cmd.admin.CompanyReadLightCmd">
            <forward name="Success" path="/contacts/CompanyInfo.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Contact/REST" type="com.piramide.elwis.web.bmapp.action.contacts.ContactRESTAction" parameter="dto(addressId)"
                scope="request" validate="false">
            <forward name="List" path="/Contact/List.do"/>
            <forward name="AddressRest" path="/Address/REST.do"/>
            <forward name="ContactPersonRest" path="/ContactPerson/REST.do"/>
        </action>

        <action path="/Contact/List"
                type="com.piramide.elwis.web.bmapp.action.contacts.ContactMainListRESTAction"
                name="listForm" scope="request" parameter="contacts@allAddressList" roles="CONTACT.VIEW"
                className="com.piramide.elwis.web.fantabulous.WhiteSpaceToWildcardSearchParameterMapping">
            <set-property property="parameterName" value="contactSearchName" />
            <forward name="Success" path="/contacts/ContactJsonList.jsp" />
        </action>

        <action path="/Contact/Recent/List"
                type="com.piramide.elwis.web.bmapp.action.ListRESTAction"
                name="listForm" scope="request" parameter="contacts@contactRecentList" roles="CONTACT.VIEW"
                className="com.piramide.elwis.web.fantabulous.WhiteSpaceToWildcardSearchParameterMapping">
            <set-property property="parameterName" value="contactSearchName" />
            <forward name="Success" path="/contacts/ContactRecentJsonList.jsp" />
        </action>

        <action path="/Address/REST" type="com.piramide.elwis.web.bmapp.action.DefaultRESTAction" parameter="dto(addressId)"
                scope="request" validate="false">
            <forward name="List" path="/Contact/List.do"/>
            <forward name="Get" path="/Address/Forward/Update.do"/>
            <!--<forward name="Get" path="/Address/Update.do"/>-->
            <forward name="Create" path="/error.jsp"/>
            <forward name="Update" path="/Address/Update.do"/>
            <forward name="Delete" path="/error.jsp"/>
        </action>

        <action path="/Address/Forward/Create" type="org.apache.struts.actions.ForwardAction"
                name="form" parameter="/bmapp/contacts/ContactCatalogsJson.jsp" validate="false" scope="request"/>

        <action path="/Address/Create" type="com.piramide.elwis.web.bmapp.action.contacts.AddressCreateRESTAction"
                name="addressForm"
                scope="request" input="/contacts/ContactJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.AddressCreateCmd" roles="CONTACT.CREATE">
            <!--<forward name="addtelecom" path="/PersonDetailForCreate.jsp"/>-->
            <forward name="Success" path="/SuccessJson.jsp"/>
            <!--<forward name="Cancel" path="/Search.do" redirect="true"/>-->
            <!--<forward name="FailModuleEntries" path="/Search.do" redirect="true"/>-->
        </action>

        <action path="/Address/Forward/Update"
                type="com.piramide.elwis.web.contactmanager.action.AddressForwardAction"
                name="addressForm"
                scope="request"
                input="/FailJson.jsp"
                validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.AddressCmd"
                roles="CONTACT.VIEW">
            <forward name="Success" path="/contacts/ContactJson.jsp"/>
        </action>

        <action path="/Address/Update"
                type="com.piramide.elwis.web.bmapp.action.contacts.AddressUpdateRESTAction"
                name="addressForm"
                scope="request"
                input="/contacts/ContactJson.jsp"
                validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.AddressUpdateCmd" roles="CONTACT.UPDATE">
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/Address/Delete" type="com.piramide.elwis.web.contactmanager.action.AddressDeleteAction"
                name="addressForm"
                scope="request" input="/contacts/ContactJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.AddressDeleteCmd" roles="CONTACT.DELETE">
            <forward name="Success" path="/SuccessJson.jsp" redirect="true"/>
            <forward name="Fail" path="/FailJson.jsp"/>
            <!--<forward name="Cancel" path="/Person/Forward/Update.do" redirect="true"/>-->
            <forward name="Referenced" path="/FailJson.jsp"/>
        </action>


        <action path="/ContactPerson/List" type="com.piramide.elwis.web.bmapp.action.contacts.ContactListRESTAction"
                name="listForm" scope="request" parameter="contacts@contactPersonList" roles="CONTACTPERSON.VIEW">
            <forward name="Success" path="/contacts/ContactPersonJsonList.jsp"/>
        </action>

        <action path="/ContactPerson/REST" type="com.piramide.elwis.web.bmapp.action.DefaultRESTAction" parameter="dto(contactPersonId)"
                scope="request" validate="false">
            <forward name="List" path="/Contact/List.do"/>
            <forward name="Get" path="/ContactPerson/Forward/Update.do"/>
            <forward name="Create" path="/error.jsp"/>
            <forward name="Update" path="/error.jsp"/>
            <forward name="Delete" path="/error.jsp"/>
        </action>

        <action path="/ContactPerson/Forward/Create" type="org.apache.struts.actions.ForwardAction" name="form"
                scope="request" validate="false" parameter="/bmapp/contacts/ContactPersonCatalogsJson.jsp"/>

        <action path="/ContactPerson/Create" type="com.piramide.elwis.web.bmapp.action.contacts.ContactPersonCreateRESTAction"
                name="contactPersonForm"
                scope="request" input="/contacts/ContactPersonJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ContactPersonCreateCmd" roles="CONTACTPERSON.CREATE"
                className="com.piramide.elwis.web.common.mapping.CheckEntriesMapping">
            <set-property property="mainTable" value="address"/>
            <forward name="Fail" path="/FailJson.jsp"/>
            <forward name="Success" path="/SuccessJson.jsp" redirect="true"/>
            <forward name="redirect" path="/FailJson.jsp"/>
        </action>

        <action path="/ContactPerson/Forward/Update"
                type="com.piramide.elwis.web.contactmanager.action.ContactPersonUpdateForwardAction" name="contactPersonForm"
                scope="request" input="/FailJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ContactPersonCmd" roles="CONTACTPERSON.VIEW">
            <forward name="Success" path="/contacts/ContactPersonJson.jsp"/>
        </action>

        <action path="/ContactPerson/Update" type="com.piramide.elwis.web.bmapp.action.contacts.ContactPersonUpdateRESTAction"
                name="contactPersonForm"
                scope="request" input="/contacts/ContactPersonJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ContactPersonUpdateCmd" roles="CONTACTPERSON.UPDATE">
            <forward name="Success" path="/SuccessJson.jsp"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>

        <action path="/ContactPerson/SearchContact"
                type="com.piramide.elwis.web.bmapp.action.contacts.ContactListRESTAction"
                name="listForm" scope="request" validate="false"
                parameter="contacts@personSearchList_CPerson" roles="CONTACTPERSON.CREATE">
            <forward name="Success" path="/contacts/ContactToAddAsContactPersonJsonList.jsp"/>
        </action>

        <action path="/ContactPerson/Import"
                type="com.piramide.elwis.web.contactmanager.action.ContactPersonImportAction" name="contactPersonForm"
                scope="request" input="/FailJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ImportPersonAddressCmd" roles="CONTACTPERSON.CREATE">
            <forward name="Success" path="/contacts/ContactToAddAsContactPersonJson.jsp"/>
        </action>

        <action path="/ContactPerson/Forward/Delete"
                type="com.piramide.elwis.web.bmapp.action.contacts.ContactManagerRESTAction" name="contactPersonForm"
                scope="request" input="/FailJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ContactPersonCmd" roles="CONTACTPERSON.DELETE">
            <forward name="Success" path="/contacts/ContactPersonJson.jsp"/>
        </action>
        <action path="/ContactPerson/Delete" type="com.piramide.elwis.web.bmapp.action.contacts.ContactManagerRESTAction"
                name="contactPersonForm"
                scope="request" input="/FailJson.jsp" validate="false"
                parameter="com.piramide.elwis.cmd.contactmanager.ContactPersonDeleteCmd" roles="CONTACTPERSON.DELETE">
            <forward name="Success" path="/SuccessJson.jsp" redirect="true"/>
            <forward name="Fail" path="/FailJson.jsp"/>
        </action>


        <!--catalogs-->
        <action path="/Country/City" type="org.apache.struts.actions.ForwardAction"
                name="form" parameter="/bmapp/contacts/CountryCityJson.jsp" validate="false" scope="request"/>

        <!--webmail-->
        <action path="/Webmail/REST" type="com.piramide.elwis.web.bmapp.action.DefaultRESTAction" parameter="dto(mailId)"
                scope="request" validate="false">
            <forward name="List" path="/Mail/MailTray.do"/>
            <forward name="Get" path="/Mail/ReadEmail.do"/>
            <forward name="Create" path="/error.jsp"/>
            <forward name="Update" path="/error.jsp"/>
            <forward name="Delete" path="/error.jsp"/>
