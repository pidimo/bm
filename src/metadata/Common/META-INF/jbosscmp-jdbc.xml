<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jbosscmp-jdbc PUBLIC "-//JBoss//DTD JBOSSCMP-JDBC 4.0//EN"
        "http://www.jboss.org/j2ee/dtd/jbosscmp-jdbc_4_0.dtd">
<jbosscmp-jdbc>
    <defaults>
        <datasource>java:/elwisDS
        </datasource>
    </defaults>
    <enterprise-beans>
        <entity>
            <ejb-name>UserSession</ejb-name>
            <table-name>usersession</table-name>
            <cmp-field>
                <field-name>userId</field-name>
                <column-name>userid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>statusName</field-name>
                <column-name>statusname</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>module</field-name>
                <column-name>module</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>UserSessionParam</ejb-name>
            <table-name>usersessionparam</table-name>
            <cmp-field>
                <field-name>userId</field-name>
                <column-name>userid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>statusName</field-name>
                <column-name>statusname</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>module</field-name>
                <column-name>module</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>paramName</field-name>
                <column-name>paramname</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
                <column-name>value</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
                <column-name>type</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>SystemConstant</ejb-name>
            <table-name>systemconstant</table-name>
            <cmp-field>
                <field-name>name</field-name>
                <column-name>name</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>resourceKey</field-name>
                <column-name>resourcekey</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>description</field-name>
                <column-name>description</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
                <column-name>type</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
                <column-name>value</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>Sequence</ejb-name>
            <row-locking>true</row-locking>
            <table-name>sequence</table-name>
            <cmp-field>
                <field-name>index</field-name>
                <column-name>sequencenumber</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>name</field-name>
                <column-name>name</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>FileFreeText</ejb-name>
            <table-name>freetext</table-name>
            <cmp-field>
                <field-name>fileId</field-name>
                <column-name>freetextid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
                <column-name>freetextvalue</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>version</field-name>
                <column-name>version</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
                <column-name>freetexttype</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>Attachment</ejb-name>
            <table-name>attachment</table-name>
            <cmp-field>
                <field-name>attachmentId</field-name>
                <column-name>attachmentid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>attachmentType</field-name>
                <column-name>attachmenttype</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>contentType</field-name>
                <column-name>contenttype</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>fileName</field-name>
                <column-name>filename</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>fileSize</field-name>
                <column-name>filesize</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>freeTextId</field-name>
                <column-name>freetextid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>version</field-name>
                <column-name>version</column-name>
            </cmp-field>
        </entity>
    </enterprise-beans>
    <relationships>
        <ejb-relation>
            <ejb-relation-name>UserSession - UserSessionParam</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>UserSession</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>userId</field-name>
                        <column-name>userid</column-name>
                    </key-field>
                    <key-field>
                        <field-name>statusName</field-name>
                        <column-name>statusname</column-name>
                    </key-field>
                    <key-field>
                        <field-name>module</field-name>
                        <column-name>module</column-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>UserSessionParam</ejb-relationship-role-name>
                <key-fields/>
                <batch-cascade-delete/>
            </ejb-relationship-role>
        </ejb-relation>
        <ejb-relation>
            <ejb-relation-name>Attachment - FileFreeText</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>attachment</ejb-relationship-role-name>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>fileFreeText</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>fileId</field-name>
                        <column-name>freetextid</column-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
        </ejb-relation>
    </relationships>
</jbosscmp-jdbc>
