<?xml version="1.0" encoding="UTF-8"?>

<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd"
         version="2.1">

    <enterprise-beans>
        <entity>
            <ejb-name>UserSession</ejb-name>
            <local-home>com.piramide.elwis.domain.common.session.UserSessionHome</local-home>
            <local>com.piramide.elwis.domain.common.session.UserSession</local>
            <ejb-class>com.piramide.elwis.domain.common.session.UserSessionBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>com.piramide.elwis.domain.common.session.UserSessionPK</prim-key-class>
            <reentrant>false</reentrant>

            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>UserSession</abstract-schema-name>
            <cmp-field>
                <field-name>userId</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>statusName</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>module</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
            </cmp-field>
            <query>
                <query-method>
                    <method-name>findByUserId</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[SELECT OBJECT(us) FROM UserSession as us WHERE us.userId=?1]]>
                </ejb-ql>
            </query>
        </entity>
        <entity>
            <ejb-name>UserSessionParam</ejb-name>
            <local-home>com.piramide.elwis.domain.common.session.UserSessionParamHome</local-home>
            <local>com.piramide.elwis.domain.common.session.UserSessionParam</local>
            <ejb-class>com.piramide.elwis.domain.common.session.UserSessionParamBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>com.piramide.elwis.domain.common.session.UserSessionParamPK</prim-key-class>
            <reentrant>false</reentrant>

            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>UserSessionParam</abstract-schema-name>
            <cmp-field>
                <field-name>userId</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>statusName</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>module</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>paramName</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
            </cmp-field>
            <query>
                <query-method>
                    <method-name>findParamByType</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.Integer</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[SELECT OBJECT(a) FROM UserSessionParam as a WHERE a.userId=?1 AND a.statusName=?2 AND a.module=?3 AND a.type=?4]]>
                </ejb-ql>
            </query>
            <query>
                <query-method>
                    <method-name>findParamByNotEqualType</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.Integer</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[SELECT OBJECT(a) FROM UserSessionParam as a WHERE a.userId=?1 AND a.statusName=?2 AND a.module=?3 AND a.type<>?4]]>
                </ejb-ql>
            </query>
            <query>
                <description>
                    Find UserSessionParams entities, where paramName
                    attribute is distinct of fourth parameter of the method.
                </description>
                <query-method>
                    <method-name>findParamsByNotEqualParamName</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[
                    SELECT OBJECT(a)
                    FROM UserSessionParam as a
                    WHERE a.userId=?1 AND a.statusName=?2 AND a.module=?3 AND a.paramName <> ?4
                    ]]>
                </ejb-ql>
            </query>
            <query>
                <description>
                    Find UserSessionParams entities, where paramName
                    attribute is equal of fourth parameter of the method.
                </description>
                <query-method>
                    <method-name>findParamsByEqualParamName</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                        <method-param>java.lang.String</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[
                    SELECT OBJECT(a)
                    FROM UserSessionParam as a
                    WHERE a.userId=?1 AND a.statusName=?2 AND a.module=?3 AND a.paramName = ?4
                    ]]>
                </ejb-ql>
            </query>
        </entity>
        <entity>
            <ejb-name>SystemConstant</ejb-name>
            <local-home>com.piramide.elwis.domain.common.config.SystemConstantHome</local-home>
