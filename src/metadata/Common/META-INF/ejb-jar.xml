            <ejb-class>com.piramide.elwis.domain.common.config.SystemConstantBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>com.piramide.elwis.domain.common.config.SystemConstantPK</prim-key-class>
