                <multiplicity>One</multiplicity>
                <relationship-role-source>
                    <ejb-name>UserSession</ejb-name>
                </relationship-role-source>
                <cmr-field>
                    <cmr-field-name>params</cmr-field-name>
                    <cmr-field-type>java.util.Collection</cmr-field-type>
                </cmr-field>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>UserSessionParam</ejb-relationship-role-name>
                <multiplicity>Many</multiplicity>
                <cascade-delete/>
                <relationship-role-source>
                    <ejb-name>UserSessionParam</ejb-name>
                </relationship-role-source>
                <cmr-field>
                    <cmr-field-name>userSession</cmr-field-name>
                </cmr-field>
            </ejb-relationship-role>
