            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>SystemConstant</abstract-schema-name>
            <cmp-field>
                <field-name>name</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>resourceKey</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>description</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
            </cmp-field>
            <query>
                <query-method>
                    <method-name>findConstantsByType</method-name>
                    <method-params>
                        <method-param>java.lang.String</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[SELECT OBJECT(sc) FROM SystemConstant as sc WHERE sc.type=?1]]>
                </ejb-ql>
            </query>
        </entity>
        <entity>
            <ejb-name>Sequence</ejb-name>
            <local-home>com.piramide.elwis.domain.common.sequence.SequenceHome</local-home>
            <local>com.piramide.elwis.domain.common.sequence.Sequence</local>
            <ejb-class>com.piramide.elwis.domain.common.sequence.SequenceBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>false</reentrant>

            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>Sequence</abstract-schema-name>
            <cmp-field>
                <field-name>name</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>index</field-name>
            </cmp-field>
            <primkey-field>name</primkey-field>
        </entity>
        <entity>
            <ejb-name>FileFreeText</ejb-name>
            <local-home>com.piramide.elwis.domain.common.FileFreeTextHome</local-home>
            <local>com.piramide.elwis.domain.common.FileFreeText</local>
            <ejb-class>com.piramide.elwis.domain.common.FileFreeTextBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>false</reentrant>
            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>FileFreeText</abstract-schema-name>
            <cmp-field>
                <field-name>fileId</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>value</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>version</field-name>
            </cmp-field>
            <cmp-field>
                <field-name>type</field-name>
            </cmp-field>
            <primkey-field>fileId</primkey-field>
            <query>
                <query-method>
                    <method-name>findByPrimaryKeyAndType</method-name>
                    <method-params>
                        <method-param>java.lang.Integer</method-param>
                        <method-param>java.lang.Integer</method-param>
                    </method-params>
                </query-method>
                <ejb-ql>
                    <![CDATA[SELECT OBJECT(fft) FROM FileFreeText as fft WHERE fft.fileId=?1 AND fft.type=?2]]>
                </ejb-ql>
            </query>
        </entity>
        <entity>
            <ejb-name>Attachment</ejb-name>
            <local-home>com.piramide.elwis.domain.common.AttachmentHome</local-home>
            <local>com.piramide.elwis.domain.common.Attachment</local>
            <ejb-class>com.piramide.elwis.domain.common.AttachmentBean</ejb-class>
            <persistence-type>Container</persistence-type>
            <prim-key-class>java.lang.Integer</prim-key-class>
            <reentrant>false</reentrant>

            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>Attachment</abstract-schema-name>
