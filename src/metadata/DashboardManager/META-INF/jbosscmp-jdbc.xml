<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jbosscmp-jdbc PUBLIC "-//JBoss//DTD JBOSSCMP-JDBC 4.0//EN"
        "http://www.jboss.org/j2ee/dtd/jbosscmp-jdbc_4_0.dtd">

<jbosscmp-jdbc>
    <defaults>
        <datasource>java:/elwisDS
        </datasource>
    </defaults>
    <enterprise-beans>
        <entity>
            <ejb-name>AdminComponent</ejb-name>
            <table-name>dashbadmcomp</table-name>
            <cmp-field>
                <field-name>containerId</field-name>
                <column-name>dashbcontainerid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>componentId</field-name>
                <column-name>componentid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>columnY</field-name>
                <column-name>columny</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyid</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>rowX</field-name>
                <column-name>rowx</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>xmlComponentId</field-name>
                <column-name>xmlcomponentid</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>DashboardContainer</ejb-name>
            <table-name>dashbcontainer</table-name>
            <cmp-field>
                <field-name>dashboardContainerId</field-name>
                <column-name>dashbcontainerid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>userId</field-name>
                <column-name>userid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>Component</ejb-name>
            <table-name>dashbcomponent</table-name>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>xmlComponentId</field-name>
                <column-name>xmlcomponentid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>componentId</field-name>
                <column-name>componentid</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>Filter</ejb-name>
            <table-name>dashbfilter</table-name>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>filterId</field-name>
                <column-name>filterid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>isRange</field-name>
                <column-name>isrange</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>name</field-name>
                <column-name>name</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>val</field-name>
                <column-name>value</column-name>
            </cmp-field>
        </entity>
        <entity>
            <ejb-name>ComponentColumn</ejb-name>
            <table-name>dashbcompcolumn</table-name>
            <cmp-field>
                <field-name>companyId</field-name>
                <column-name>companyid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>componentColumnId</field-name>
                <column-name>compcolumnid</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>ord</field-name>
                <column-name>ord</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>position</field-name>
                <column-name>position</column-name>
            </cmp-field>
            <cmp-field>
                <field-name>xmlColumnId</field-name>
                <column-name>xmlcolumnid</column-name>
            </cmp-field>
        </entity>
    </enterprise-beans>
    <relationships>
        <ejb-relation>
            <ejb-relation-name>Component - Column</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>componentRole</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>componentId</field-name>
                        <column-name>componentid</column-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>columnRole</ejb-relationship-role-name>
                <key-fields/>
            </ejb-relationship-role>
        </ejb-relation>
        <ejb-relation>
            <ejb-relation-name>Component - Filter</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>componentRole</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>componentId</field-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>filterRole</ejb-relationship-role-name>
                <key-fields/>
            </ejb-relationship-role>
        </ejb-relation>
        <ejb-relation>
            <ejb-relation-name>DashboardContainer - AdminComponent</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>dashboardContainerRole</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>dashboardContainerId</field-name>
                        <column-name>dashbcontainerid</column-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>adminComponentRole</ejb-relationship-role-name>
                <key-fields/>
            </ejb-relationship-role>
        </ejb-relation>
        <ejb-relation>
            <ejb-relation-name>AdminComponent - Component</ejb-relation-name>
            <ejb-relationship-role>
                <ejb-relationship-role-name>AdminComponentRole</ejb-relationship-role-name>
                <key-fields/>
            </ejb-relationship-role>
            <ejb-relationship-role>
                <ejb-relationship-role-name>componentRole</ejb-relationship-role-name>
                <key-fields>
                    <key-field>
                        <field-name>componentId</field-name>
                        <column-name>componentid</column-name>
                    </key-field>
                </key-fields>
            </ejb-relationship-role>
        </ejb-relation>
    </relationships>
</jbosscmp-jdbc>